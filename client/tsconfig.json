{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "esnext", "dom.iterable"],
    "outDir": "dist",
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": "src",
    "paths": {
      "assets/*": ["assets/*"],
      "components/*": ["components/*"],
      "pages/*": ["pages/*"],
      "hooks/*": ["hooks/*"],
      "hok/*": ["hok/*"],
      "helpers/*": ["helpers/*"],
      "services/*": ["services/*"],
      "store/*": ["store/*"],
      "types/*": ["types/*"],
      "const/*": ["const/*"],
  },
},
  "include": ["src"],
  
}